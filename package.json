{
  "name": "mind-assignment",
  "version": "1.0.0",
  "description": "This is a program that give the user and idea of how big different types of stars are when compared to the earth and sun.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "npm run build:mk-dirs && npm run build:moveFiles && webpack",
    "build:mk-dirs": "mkdir -p ./dist/public && mkdir -p ./dist/public/images",
    "build:moveFiles": "npm run build:html && npm run build:css && npm run build:images",
    "build:html": "cp ./src/public/*.html ./dist/public/",
    "build:css": "cp ./src/public/*.css ./dist/public/",
    "build:images": "cp ./src/public/images/*.png ./dist/public/images/",
    "webpack": "webpack",
    "webpack:watch": "webpack --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Russell Murray",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "install": "^0.8.1",
    "json-loader": "^0.5.4",
    "npm": "^3.10.8",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "createjs-collection": "^0.8.2-1",
    "express": "^4.14.0",
    "lodash.curry": "^4.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
